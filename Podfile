
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'

target 'SnapkitPoc' do
  # Comment the next line if you don't want to use dynamic frameworks
  #use_frameworks!
      pod 'SnapKit', '~> 4.0'
      pod 'Alamofire', '4.9.1'
      pod 'AlamofireNetworkActivityLogger'
      pod 'ObjectMapper', '3.5.1'
      pod 'Kingfisher'

  target 'SnapkitPocTests' do
    inherit! :search_paths
    
    pod 'SnapKit', '~> 4.0'
     pod 'Alamofire', '4.9.1'
     pod 'AlamofireNetworkActivityLogger'
     pod 'ObjectMapper', '3.5.1'
     pod 'Kingfisher'
  end

  target 'SnapkitPocUITests' do
     pod 'SnapKit', '~> 4.0'
     pod 'Alamofire', '4.9.1'
     pod 'AlamofireNetworkActivityLogger'
     pod 'ObjectMapper', '3.5.1'
     pod 'Kingfisher'
  end

end
